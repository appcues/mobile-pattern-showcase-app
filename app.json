{
  "expo": {
    "name": "Showcase",
    "slug": "appcues-mobile-showcase",
    "owner": "appcues",
    "version": "1.0.1",
    "orientation": "default",
    "icon": "./assets/images/icon.png",
    "scheme": [
      "appcues-showcase",
      "appcues-16daf46b-3231-4e4a-bb3c-273a4e9100dd"
    ],
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff",
      "dark": {
        "image": "./assets/images/splash-dark.png",
        "backgroundColor": "#0B1A38"
      }
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.appcues.mobile-showcase",
      "config": {
        "usesNonExemptEncryption": false
      },
      "infoPlist": {
        "CFBundleDisplayName": "Appcues",
        "CFBundleAllowMixedLocalizations": true,
        "AppcuesUniversalLinkHostAllowList": [],
        "UIAppFonts": [
          "Mulish-Black.ttf",
          "Mulish-ExtraBold.ttf",
          "Mulish-Bold.ttf",
          "Mulish-SemiBold.ttf",
          "Mulish-Medium.ttf",
          "Mulish-Regular.ttf",
          "Mulish-Light.ttf",
          "Mulish-ExtraLight.ttf",
          "Mulish-BlackItalic.ttf",
          "Mulish-ExtraBoldItalic.ttf",
          "Mulish-BoldItalic.ttf",
          "Mulish-SemiBoldItalic.ttf",
          "Mulish-MediumItalic.ttf",
          "Mulish-Italic.ttf",
          "Mulish-LightItalic.ttf",
          "Mulish-ExtraLightItalic.ttf"
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundImage": "./assets/images/adaptive-icon-background.png"
      },
      "package": "com.appcues.mobileshowcase"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 34,
            "targetSdkVersion": 34,
            "buildToolsVersion": "34.0.0"
          }
        }
      ],
      "expo-localization"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "d12317a1-9d22-474d-8b07-e8751ee2f4e1"
      }
    }
  }
}
